{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Dynbox App",
  "version": "0.1.11",
  "identifier": "com.dynbox.app",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3001",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "plugins": {
    "deep-link": {
      "mobile": [
        { "host": "dynbox.co", "pathPrefix": ["/mobile"] }
      ],
      "desktop": {
        "schemes": ["dynbox"]
      }
    },
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDlEMTNCMUUzMjlGQUEzNTgKUldSWW8vb3A0N0VUblhGVi9WckZoL3Q1TW8xR3NoUTF0c1lIbG1tRjc1RGoyQXRKS2p0WUNSZWQK",
      "endpoints": [
        "https://github.com/codyadam/dynbox-app/releases/latest/download/latest.json"
      ]
    }
  },
  "app": {
    "windows": [
      {
        "title": "Dynbox",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": false
      }
    ],

    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [
      "bin/rclone-dynbox"
    ],
    "createUpdaterArtifacts": true
    
  }
}
